<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="ore_ore_view_form_inherit" model="ir.ui.view">
        <field name="model">ore.ore</field>
        <field name="inherit_id" ref="ore.ore_ore_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//label[@for='street']" position="before">
                <field name="company_id" required="0" groups="base.group_no_one" />
                <field name="partner_id" readonly="1" required="0" groups="base.group_no_one" />
            </xpath>
        </field>
    </record>
</odoo>
